{
  "name": "expo-push-notifications-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "mobile",
    "backend",
    "admin"
  ],
  "scripts": {
    "mobile": "cd mobile && pnpm start",
    "mobile:tunnel": "cd mobile && pnpm start:tunnel",
    "mobile:lan": "cd mobile && pnpm start:lan",
    "mobile:reset": "cd mobile && pnpm start --clear",
    "mobile:reset-project": "cd mobile && pnpm reset-project",
    "backend": "cd backend && node server.js",
    "admin": "cd admin && python -m http.server 8080 || python3 -m http.server 8080",
    "serve-apk": "node scripts/serve-apk.js"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
